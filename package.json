{
  "name": "elderjs-template",
  "version": "1.3.2",
  "description": "This is a project template for [Elder.js](https://elderguide.com/tech/elderjs/) apps. The template lives at https://github.com/elderjs/template and the Elder.js source is here: https://github.com/elderjs/elderjs",
  "main": "./src/build.js",
  "scripts": {
    "start": "npm run dev",
    "build": "node ./src/cleanPublic.js && npm run build:rollup && npm run build:html",
    "build:rollup": "rollup -c",
    "build:html": "node ./src/build.js",
    "dev": "rollup  -c -w --no-watch.clearScreen",
    "serve": "node ./src/cleanPublic.js && npm run build:rollup && NODE_ENV=production node ./src/server.js",
    "esbuild": "node esbuild",
    "eslint": "eslint --ext .js .",
    "eslint:fix": "eslint --fix --ext .js .",
    "prettier:svelte": "prettier --write --plugin-search-dir=. ./**/*.svelte"
  },
  "type": "commonjs",
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@elderjs/elderjs": "^1.5.5",
    "@elderjs/plugin-browser-reload": "^0.1.44",
    "@elderjs/plugin-seo-check": "0.1.37",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "dotenv": "^8.6.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.7",
    "intersection-observer": "^0.12.0",
    "polka": "^0.5.2",
    "sirv": "^1.0.12",
    "svelte": "^3.41.0",
    "svelte-preprocess": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/runtime": "^7.14.8",
    "@elderjs/plugin-markdown": "^0.1.44",
    "@elderjs/plugin-sitemap": "^1.2.0",
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@remark-embedder/cache": "^1.0.0",
    "@remark-embedder/core": "^1.4.2",
    "@remark-embedder/transformer-oembed": "^1.0.2",
    "autoprefixer": "^10.3.1",
    "chota": "^0.8.0",
    "dayjs": "^1.10.6",
    "escape-html": "^1.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rehype-katex": "^5.0.0",
    "rehype-mathjax": "^3.1.0",
    "rehype-raw": "^5.1.0",
    "remark-footnotes": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-kroki-plugin": "^0.1.1",
    "remark-math": "^4.0.0",
    "remark-mermaid-dataurl": "^2.0.4",
    "remark-rehype": "^8.1.0",
    "rollup": "^2.55.1",
    "svelte-chota": "^1.8.6",
    "svelte-seo": "^1.2.1",
    "svelte-time": "^0.4.0"
  },
  "author": "Nick Reese",
  "license": "MIT",
  "files": [
    "index.js",
    "elderjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elderjs/template.git"
  },
  "keywords": [
    "svelte",
    "elderjs",
    "ssr",
    "static",
    "site",
    "generator",
    "seo",
    "ssg",
    "template",
    "partial",
    "hydration"
  ],
  "bugs": {
    "url": "https://github.com/Elderjs/template/issues"
  },
  "homepage": "https://github.com/Elderjs/template#readme"
}
